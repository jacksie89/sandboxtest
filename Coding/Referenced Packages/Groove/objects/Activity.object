        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>indicates whether Groove detected at least one reply to this Email (Task). This only works if the respective email was logged (e.g. via Groove Email Sync or Groove for Gmail).</description>
        <externalId>false</externalId>
        <label>Replied</label>
        <type>Checkbox</type>
    </fields>
